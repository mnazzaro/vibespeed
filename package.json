    "lint-staged": "^15.2.11",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "~5.9.2",
    "vite": "^5.0.12"
  },
  "dependencies": {
    "@octokit/auth-app": "^8.1.0",
    "@octokit/rest": "^22.0.0",
